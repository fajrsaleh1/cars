<p id="notice"><%= notice %></p>

<h1>Person People</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person_people.each do |person_person| %>
      <tr>
        <td><%= person_person.first_name %></td>
        <td><%= person_person.last_name %></td>
        <td><%= person_person.age %></td>
        <td><%= link_to 'Show', person_person %></td>
        <td><%= link_to 'Edit', edit_person_person_path(person_person) %></td>
        <td><%= link_to 'Destroy', person_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person Person', new_person_person_path %>
